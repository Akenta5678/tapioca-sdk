{
    "version": "1.1.24",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=12"
    },
    "scripts": {
        "start": "tsdx watch",
        "build": "tsdx build && npm run copy-dts && npm run copy-sol",
        "test": "tsdx test --passWithNoTests",
        "lint": "npx eslint .", 
        "prepare": "npm run build",
        "deploy": "npm run prepare && npm publish",
        "copy-dts": "copyfiles -u 1 \"src/**/*.d.ts\" dist",
        "copy-sol": "copyfiles -u 1 \"src/**/*.sol\" dist"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npx eslint ."
        }
    },
    "name": "tapioca-sdk",
    "author": "0xRektora",
    "module": "dist/tapioca-sdk.esm.js",
    "dependencies": {
        "ethers": "^6.0.8",
        "lodash": "^4.17.21",
        "lodash-es": "^4.17.21",
        "typechain": "^8.1.1",
        "write-json-file": "5.0.0"
    },
    "devDependencies": {
        "@types/chai": "^4.3.4",
        "@types/jest": "^29.4.0",
        "@types/lodash": "^4.14.191",
        "@typescript-eslint/eslint-plugin": "^5.54.0",
        "@typescript-eslint/parser": "^5.54.0",
        "chai": "^4.3.7",
        "copyfiles": "^2.4.1",
        "eslint": "^8.35.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.3",
        "prettier": "^2.8.4",
        "tsdx": "^0.14.1",
        "tslib": "^2.5.0",
        "typescript": "^4.9.5"
    },
    "resolutions": {
        "tsdx/typescript": "^4.6.3"
    }
}
